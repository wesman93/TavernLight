MainWindow
  id: jumpMain
  size: 800 800
  @onEscape: modules.jump.closing()

  Panel
    id: optionsTabContent
    anchors.top: allTabs.top
    anchors.left: allTabs.left
    anchors.right: allTabs.right
    anchors.bottom: allTabs.bottom
    margin-top: 20
    margin-bottom: 20

  Button
    id: jumpButton
    !text: tr('Jump!')
    width: 50
    @onClick: modules.jump.jump()
